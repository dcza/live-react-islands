{
  "name": "@live-react-islands/vite-plugin-ssr",
  "version": "0.1.0",
  "description": "Vite plugin for LiveReactIslands SSR development server",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "ssr",
    "liveview",
    "react",
    "islands"
  ],
  "author": "David Czaplinski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcza/live-react-islands.git",
    "directory": "packages/js/vite-plugin-ssr"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.7"
  }
}
